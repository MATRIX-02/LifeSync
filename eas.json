{
	"cli": {
		"version": ">= 5.0.0",
		"appVersionSource": "local"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"android": {
				"buildType": "apk"
			}
		},
		"preview": {
			"android": {
				"buildType": "apk"
			},
			"distribution": "internal",
			"cache": {
				"disabled": false
			}
		},
		"production": {
			"android": {
				"buildType": "apk"
			},
			"cache": {
				"disabled": false
			}
		}
	},
	"submit": {
		"production": {}
	}
}
